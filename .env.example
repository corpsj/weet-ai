# Gemini API Key (필수)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Upscale Backend URL (선택사항)
# 로컬 개발: http://localhost:8000
# Vercel 배포 + Cloudflare Tunnel: https://your-backend.example.com
UPSCALE_BACKEND_URL=http://localhost:8000

# Mock Upscale Mode (프론트엔드 테스트용)
# true로 설정하면 백엔드 없이도 업스케일링 UI 테스트 가능
# 실제 업스케일은 되지 않고 원본 이미지를 그대로 반환
NEXT_PUBLIC_MOCK_UPSCALE=false
